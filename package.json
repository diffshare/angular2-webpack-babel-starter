{
  "name": "angular2-webpack-slm-starter",
  "version": "0.0.0",
  "description": "A skeleton Angular 2 app built with Slm and Webpack.",
  "scripts": {
    "rimraf": "rimraf",
    "clean": "npm cache clean && npm run rimraf -- node_modules dist",
    "watch": "webpack --watch --progress --profile --coloes --display-error-details --display-cached",
    "build": "webpack --progress --profile --colors --display-error-details --display-cached",
    "server": "webpack-dev-server --progress --profile --colors --display-error-details --display-cached --content-base src/",
    "start": "npm run server",
    "test": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diffshare/angular2-webpack-slm-starter.git"
  },
  "author": "diffshare",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/diffshare/angular2-webpack-slm-starter/issues"
  },
  "homepage": "https://github.com/diffshare/angular2-webpack-slm-starter#readme",
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "^0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/platform-server": "2.0.0-rc.5",
    "@angular/router": "3.0.0-beta.2",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.17"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.32",
    "@types/node": "^6.0.38",
    "awesome-typescript-loader": "^2.2.1",
    "css-loader": "^0.23.1",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.7.2",
    "node-sass": "^3.7.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.1",
    "sass-loader": "^3.1.2",
    "slm": "git://github.com/diffshare/slm.git",
    "slm-loader": "^0.1.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.0",
    "typescript": "^2.0.0-beta",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.14.1"
  }
}
